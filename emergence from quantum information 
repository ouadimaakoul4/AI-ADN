\section{Concrete Example: 2D SYK/JT Correspondence}

\subsection{Setup}

We focus on the Sachdev-Ye-Kitaev (SYK) model \citep{sachdev1993, kitaev2015} as a concrete quantum system:

\begin{equation}
H_{\mathrm{SYK}} = i^{q/2}\sum_{1 \leq i_1 < \cdots < i_q \leq N} J_{i_1\cdots i_q} \chi_{i_1} \cdots \chi_{i_q}
\end{equation}
where $\chi_i$ are $N$ Majorana fermions with $\{\chi_i, \chi_j\} = \delta_{ij}$, and $J_{i_1\cdots i_q}$ are random couplings with variance $\langle J^2 \rangle = \frac{(q-1)!J^2}{N^{q-1}}$.

The thermofield double (TFD) state at inverse temperature $\beta$ is:
\begin{equation}
\ket{\mathrm{TFD}(\beta)} = \frac{1}{\sqrt{Z(\beta)}}\sum_n e^{-\beta E_n/2}\ket{n}_L \otimes \ket{n}_R
\end{equation}

\subsection{QFIM Computation}

Parameters: $\lambda^a = (\beta, \tau)$ where $\tau$ is Euclidean time. The QFIM components are:

\begin{align}
F_{\beta\beta} &= \langle (H - \langle H\rangle)^2 \rangle_\beta \\
F_{\tau\tau} &= -\partial_\tau^2 \log G(\tau)|_{\tau=0} \\
F_{\beta\tau} &= \langle H \partial_\tau \mathcal{O}(\tau) \rangle_\beta
\end{align}

For SYK at low temperature, these can be computed using the Schwarzian effective action:
\begin{equation}
Z_{\mathrm{SYK}} \approx \int \mathcal{D} f \, e^{-S_{\mathrm{Sch}}[f]}, \quad S_{\mathrm{Sch}}[f] = -\frac{N\alpha_S}{J}\int d\tau \{f, \tau\}
\end{equation}
where $\{f, \tau\} = \frac{f'''}{f'} - \frac{3}{2}\left(\frac{f''}{f'}\right)^2$ is the Schwarzian derivative.

\subsection{Connection to JT Gravity}

Jackiw-Teitelboim (JT) gravity action:
\begin{equation}
I_{\mathrm{JT}} = \frac{S_0}{4\pi}\left[\int d^2x \sqrt{-g} R + 2\int_{\partial} K\right] + \int d^2x \sqrt{-g} \phi(R + 2)
\end{equation}

The metric in Euclidean signature:
\begin{equation}
ds^2 = \frac{d\tau^2 + d\sigma^2}{\sin^2\sigma}, \quad 0 < \sigma < \pi
\end{equation}

\textbf{Research Task 1:} Find embedding $\varphi: (\tau, \sigma) \mapsto (\beta(\tau,\sigma), \tau(\tau,\sigma))$ such that:
\begin{equation}
g_{\mu\nu}^{\mathrm{JT}}(\tau,\sigma) = \frac{\partial \lambda^a}{\partial x^\mu}\frac{\partial \lambda^b}{\partial x^\nu}F_{ab}^{\mathrm{SYK}}(\lambda(\tau,\sigma))
\end{equation}

Preliminary Ansatz: $\beta(\tau,\sigma) = \beta_0 e^{-\kappa\sigma}$, $\tau(\tau,\sigma) = \tau$, with $\kappa$ determined by matching.

\subsection{Numerical Implementation}

\begin{verbatim}
import numpy as np
from scipy import sparse
import quimb as qu
import jax
import jax.numpy as jnp

def compute_syk_qfim(N, q, J, beta, n_samples=1000):
    """
    Compute QFIM for SYK model using Monte Carlo
    """
    # 1. Generate random couplings
    J_tensor = np.random.normal(0, J/np.sqrt(N), (n_samples, comb(N, q)))
    
    # 2. Compute Hamiltonian and thermal state
    H = construct_syk_hamiltonian(N, q, J_tensor)
    rho_beta = thermal_state(H, beta)
    
    # 3. Compute QFIM using automatic differentiation
    def loss(params):
        beta, tau = params
        rho = thermal_state(H, beta)
        # Time-evolved observable
        O_tau = evolve_observable(O, H, tau)
        return -jnp.log(jnp.trace(rho @ O_tau))
    
    # Compute Hessian of loss = QFIM
    hessian = jax.hessian(loss)
    F_ab = hessian(jnp.array([beta, 0.0]))
    
    return F_ab

def pullback_metric(F_ab, embedding_func, coords):
    """
    Compute emergent metric via pullback
    """
    # Compute Jacobian
    J = jax.jacobian(embedding_func)(coords)
    # Pullback: g = J^T F J
    g = jnp.einsum('ia,ab,jb->ij', J, F_ab, J)
    return g
\end{verbatim}

