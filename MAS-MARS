ğŸš MODULAR AERIAL FRAMEWORK

Complete Aerial System Architecture

```rust
#[derive(Clone, Serialize, Deserialize)]
struct ModularAerialSystem {
    // === SYSTEM STATUS ===
    status: AerialSystemStatus,
    
    // === CORE CAPABILITIES (Ingenuity Heritage) ===
    capabilities: AerialCapabilities {
        // Flight performance
        max_flight_time: Duration::from_secs(169),  // 169 seconds (Ingenuity max)
        max_range: Distance::meters(704.0),         // 704 meters (Ingenuity max)
        max_altitude: Distance::meters(24.0),       // 24 meters
        wind_tolerance: Velocity::meters_per_second(10.0),
        
        // Payload
        payload_mass: Mass::grams(50.0),
        payload_power: Power::watts(5.0),
        
        // Operational
        recharge_time: Duration::from_hours(24),
        operational_lifetime: Duration::from_sols(50),  // 50 flights
        
        // Navigation
        navigation: AerialNavigation(
            visual_odometry: VisualOdometry::enabled(),
            hazard_detection: HazardDetection::enabled(),
            landing_precision: Distance::meters(0.5)
        ),
    },
    
    // === MSR DECISION GATE ===
    decision_gate: MSRDecisionGate {
        expected_decision_date: Date::from_ymd(2026, 6, 30),
        current_status: DecisionStatus::PENDING,
        
        activation_criteria: vec![
            Criterion::MSRIncludesAerialAssets {
                required: true,
                confidence_threshold: 0.8,
            },
            Criterion::TechnicalReadiness {
                required_level: 0.9,
                current_level: self.calculate_readiness(),
            },
            Criterion::ResourceAvailability {
                power: Power::watts(50.0),
                mass: Mass::kilograms(2.0),
                volume: Volume::liters(10.0),
            },
            Criterion::MissionNeed {
                science_benefit: ScienceBenefit::SUBSTANTIAL,
                operational_benefit: OperationalBenefit::MODERATE,
            },
        ],
        
        fallback_strategy: FallbackStrategy {
            primary: FallbackOption::ROVER_ONLY,
            secondary: FallbackOption::DELAYED_DEPLOYMENT,
            tertiary: FallbackOption::MINIMAL_CAPABILITY,
        },
    },
    
    // === ROVER INTEGRATION ===
    rover_integration: RoverAerialIntegration {
        launch_recovery: LaunchRecoverySystem(
            deployment: DeploymentMechanism::ROVER_MOUNTED,
            recovery: RecoveryMechanism::AUTONOMOUS_DOCKING,
        ),
        
        data_exchange: DataExchangeProtocol(
            format: DataFormat::PROTOBUF,
            bandwidth: DataRate::megabits_per_second(10.0),
            latency: Duration::from_millis(100),
        ),
        
        power_management: PowerSharingSystem(
            rover_to_drone: PowerTransfer::WIRELESS,
            drone_to_rover: PowerTransfer::WIRED,
            efficiency: 0.85,
        ),
    },
    
    // === MISSION PROFILES ===
    mission_profiles: HashMap<MissionType, FlightProfile> {
        MissionType::SCOUTING => FlightProfile {
            altitude: Distance::meters(15.0),
            speed: Velocity::meters_per_second(5.0),
            pattern: FlightPattern::GRID,
            resolution: Resolution::centimeters(10.0),
            coverage: Coverage::square_meters(10000.0),
        },
        
        MissionType::RELAY => FlightProfile {
            altitude: Distance::meters(20.0),
            speed: Velocity::meters_per_second(8.0),
            pattern: FlightPattern::STATION_KEEPING,
            duration: Duration::from_minutes(30),
            range: Distance::meters(1000.0),
        },
        
        MissionType::SCIENCE => FlightProfile {
            altitude: Distance::meters(10.0),
            speed: Velocity::meters_per_second(3.0),
            pattern: FlightPattern::WAYPOINTS,
            instruments: vec![InstrumentType::CAMERA, InstrumentType::SPECTROMETER],
            data_collection: DataCollectionPlan::CONTINUOUS,
        },
    },
}

impl ModularAerialSystem {
    pub async execute_mission(&mut self, mission_request: MissionRequest) -> MissionResult {
        // Check activation status
        if !self.is_activated() {
            return self.rover_only_fallback(mission_request);
        }
        
        // Pre-flight validation
        let validation = self.validate_mission(mission_request).await?;
        
        // Generate flight plan
        let flight_plan = self.generate_flight_plan(mission_request, &validation);
        
        // Execute flight
        let flight_data = self.execute_flight(flight_plan).await?;
        
        // Process data
        let analysis = self.analyze_flight_data(flight_data);
        
        // Integrate with rover
        self.integrate_with_rover(analysis);
        
        Ok(MissionResult {
            success: true,
            data: analysis,
            resources_used: self.calculate_resources_used(),
            anomalies: self.detect_anomalies(),
            recommendations: self.generate_recommendations(&analysis),
        })
    }
    
    pub fn rover_only_fallback(&self, mission_request: MissionRequest) -> MissionResult {
        // Conservative rover-based alternative
        MissionResult {
            success: true,
            data: RoverScoutingData {
                coverage: 0.6,  // 60% of aerial coverage
                resolution: Resolution::centimeters(20.0),
                hazards_detected: self.conservative_hazard_assessment(mission_request.area),
                high_value_targets: vec![],
                confidence: 0.7,
            },
            resources_used: Resources {
                energy: Energy::watt_hours(50.0),
                time: Duration::hours(2),
                data: DataVolume::megabytes(100),
            },
            anomalies: vec![],
            recommendations: vec![
                Recommendation {
                    type: RecommendationType::AERIAL_UNAVAILABLE,
                    message: "Aerial system inactive - using rover-only scouting",
                    priority: Priority::MEDIUM,
                }
            ],
        }
    }
}
```

Aerial Operations Decision Logic

```
â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”
