Validation Test Matrix

```yaml
validation_test_suite:
  navigation:
    basic_traverse:
      description: "100m straight-line traverse"
      success_criteria: "95% path adherence, <5% energy deviation"
      test_count: 1000
      environment: "Simulated flat terrain"
      metrics: ["path_error", "energy_used", "time_taken"]
    
    obstacle_avoidance:
      description: "Navigate through obstacle field"
      success_criteria: "0 collisions, 90% completion rate"
      test_count: 500
      environment: "Simulated rocky terrain"
      metrics: ["collisions", "completion_rate", "path_efficiency"]
    
    slope_navigation:
      description: "Traverse 15Â° slope"
      success_criteria: "Maintain stability, complete traverse"
      test_count: 300
      environment: "Simulated slope"
      metrics: ["slip_ratio", "stability", "completion_time"]
    
    dust_devil_response:
      description: "Respond to dust devil detection"
      success_criteria: "Initiate response within 2 minutes"
      test_count: 200
      environment: "Simulated Jezero Crater"
      metrics: ["response_time", "safety_actions", "recovery_time"]
  
  science_autonomy:
    target_detection:
      description: "Identify science targets in imagery"
      success_criteria: "85% detection rate, <5% false positives"
      test_count: 1000
      environment: "Mars surface imagery dataset"
      metrics: ["precision", "recall", "f1_score"]
    
    instrument_coordination:
      description: "Coordinate multiple instruments"
      success_criteria: "100% task completion, optimal sequencing"
      test_count: 200
      environment: "Simulated investigation site"
      metrics: ["completion_rate", "resource_efficiency", "data_quality"]
    
    adaptive_sampling:
      description: "Adjust sampling based on initial results"
      success_criteria: "Improve science return by 20%"
      test_count: 150
      environment: "Variable science value terrain"
      metrics: ["science_value_gain", "adaptation_effectiveness"]
  
  fault_recovery:
    sensor_failure:
      description: "Recover from critical sensor failure"
      success_criteria: "Continue mission with degraded capability"
      test_count: 100
      environment: "Various sensor failure scenarios"
      metrics: ["recovery_time", "capability_loss", "mission_continuation"]
    
    power_anomaly:
      description: "Handle power system anomaly"
      success_criteria: "Enter safe mode, preserve state"
      test_count: 50
      environment: "Simulated power faults"
      metrics: ["response_time", "state_preservation", "recovery_success"]
    
    communication_loss:
      description: "Operate through communication blackout"
      success_criteria: "Maintain autonomy for 24 hours"
      test_count: 75
      environment: "No communication scenario"
      metrics: ["autonomy_duration", "data_preservation", "recovery"]
  
  system_integration:
    multi_agent_coordination:
      description: "Coordinate rover and drone operations"
      success_criteria: "Efficient task allocation, no conflicts"
      test_count: 100
      environment: "Multi-agent simulation"
      metrics: ["task_completion", "conflict_resolution", "efficiency"]
    
    end_to_end_mission:
      description: "Complete 7-sol simulated mission"
      success_criteria: "90% of objectives achieved"
      test_count: 50
      environment: "Full mission simulation"
      metrics: ["objective_completion", "resource_usage", "science_return"]
