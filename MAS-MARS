#[derive(Clone)]
struct OpenSourceFramework {
    // === REPOSITORY STRUCTURE ===
    repositories: HashMap<RepositoryType, RepositoryConfig> {
        RepositoryType::CORE => RepositoryConfig {
            name: "mars-autonomy-core",
            license: License::APACHE_2_0,
            visibility: Visibility::PUBLIC,
            maintainers: vec![
                Maintainer::new("Project Lead"),
                Maintainer::new("Technical Architect"),
                Maintainer::new("Community Manager"),
            ],
            ci_cd: CICDPipeline {
                tests: TestSuite::COMPREHENSIVE,
                security_scan: SecurityScan::ENABLED,
                nsa_compliance: NASAComplianceCheck::ENABLED,
                deployment: Deployment::AUTOMATIC,
            },
        },
        
        RepositoryType::SIMULATION => RepositoryConfig {
            name: "mars-simulation",
            license: License::BSD_3_CLAUSE,
            visibility: Visibility::PUBLIC,
            maintainers: vec![
                Maintainer::new("Simulation Lead"),
                Maintainer::new("Physics Model Expert"),
            ],
            data_assets: vec![
                DataAsset::new("HiRISE Terrain Data"),
