\node at (3,3) {$\rho \to \infty$};
\node at (3,2.5) {$R \to \infty$};
\draw[->] (2.5,3.2) -- (1,3.2);

\node at (-3,3.5) {EIG Planck Core};
\node at (-3,3) {$\rho \sim L_P^{-D}$};
\node at (-3,2.5) {$R \sim L_P^{-2}$};
\draw[->] (-2.5,3.2) -- (-1,3.2);

\draw[decorate,decoration={snake,amplitude=1mm}] (0,4) -- (0,0);
\node at (0,4.3) {Collapsing Matter};
\end{tikzpicture}
\caption{Singularity resolution via Fisher information saturation. Classical collapse (right) leads to infinite density; in EIG (left), Fisher information saturates at Planck scale, creating a finite core.}
\label{fig:singularity}
\end{figure}

